{
    "name": "svelte-outclick",
    "version": "4.1.2",
    "description": "A Svelte component that allows you to listen to the clicks that happen outside of an element.",
    "license": "MIT",
    "repository": "github:babakfp/svelte-outclick",
    "scripts": {
        "dev": "vite dev",
        "build": "vite build && pnpm package",
        "preview": "vite preview",
        "prepare": "svelte-kit sync",
        "package": "svelte-kit sync && svelte-package -i src/package && publint",
        "prepublishOnly": "pnpm package",
        "format": "prettier -w .",
        "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
        "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
    },
    "peerDependencies": {
        "svelte": ">= 5"
    },
    "devDependencies": {
        "@ianvs/prettier-plugin-sort-imports": "4.7.0",
        "@sveltejs/adapter-vercel": "6.2.0",
        "@sveltejs/kit": "2.49.2",
        "@sveltejs/package": "2.5.7",
        "@sveltejs/vite-plugin-svelte": "6.2.1",
        "@tailwindcss/vite": "4.1.17",
        "prettier": "3.7.4",
        "prettier-plugin-svelte": "3.4.0",
        "prettier-plugin-tailwindcss": "0.7.2",
        "publint": "0.3.16",
        "svelte": "5.45.8",
        "svelte-check": "4.3.4",
        "svelte-loading-bar": "2.0.4",
        "tailwindcss": "4.1.17",
        "tslib": "2.8.1",
        "typescript": "5.9.3",
        "vite": "7.2.7"
    },
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "svelte": "./dist/index.js"
        }
    },
    "types": "./dist/index.d.ts",
    "svelte": "./dist/index.js",
    "keywords": [
        "svelte",
        "outclick",
        "outside",
        "click"
    ],
    "type": "module"
}
